<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <!-- increments: run only once, fails if changed -->
    <include file="1.0/master-1.0.xml" relativeToChangelogFile="true" />
    <include file="1.1/master-1.1.xml" relativeToChangelogFile="true" />
    <include file="1.2/master-1.2.xml" relativeToChangelogFile="true" />

    <!-- run always -->
    <include file="run_always/master.xml" relativeToChangelogFile="true" />
</databaseChangeLog>